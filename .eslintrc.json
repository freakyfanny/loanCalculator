{
    "extends": [
      "next/core-web-vitals",
      "eslint:recommended"
    ],
    "rules": {
      "react/react-in-jsx-scope": "off",
      "react/jsx-uses-react": "off",
      "react/jsx-uses-vars": "warn",
      "no-unused-vars": "warn",
      "no-console": "warn",
      "react/prop-types": "off",
      "react/jsx-no-target-blank": "off",
      "import/no-anonymous-default-export": "off"
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    }
  }
  